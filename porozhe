class school:
def__init__(year,teacher,student_one,student_two,nomre_ yek,nomre_do):
year.teacher=teacher
year.student_one=student_one
year.student_two=student_two
year.nomre_yek=nomre_yek
year.nomre_do=nomre_do
return(year.nomre_yek+year.nomre_two)/2

